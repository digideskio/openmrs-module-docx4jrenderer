<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>docx4jrenderer</id>
	<name>Docx4j Renderer</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.docx4jrenderer</package>
	<author>jmiranda</author>
	<description>
		Renders docx4j templates.
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/docx4jrenderer/update.rdf</updateURL>
	<require_version>1.8.0</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.docx4jrenderer.Docx4jRendererActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.docx4jrenderer.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present 
	<mappingFiles>
				
		Docx4jRenderer.hbm.xml
				
	</mappingFiles>
	-->

	<!-- Internationalization -->
	<!-- All message codes should start with docx4jrenderer. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

